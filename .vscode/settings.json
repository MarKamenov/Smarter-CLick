{
	"git.ignoreLimitWarning": true,
	"tslint.configFile": "tslint.ide.json",
	"tslint.alwaysShowRuleFailuresAsWarnings": true,
	"typescript.tsdk": "./node_modules/typescript/lib",
	"cSpell.words": [
		"Lifecycle",
		"Mobx",
		"Unmount",
		"aggs",
		"crosshair",
		"datapoint",
		"docvalue",
		"drilldown",
		"drilldowns",
		"guid",
		"mobx"
	],
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		{
			"language": "typescript",
			"autoFix": true
		},
		{
			"language": "typescriptreact",
			"autoFix": true
		}
	]
}
